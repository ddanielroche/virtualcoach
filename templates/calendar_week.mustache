{{!
    @template virtualcoach/calendar_week

    Calendar week view for blcoks.

    The purpose of this template is to render the main, detailed weekly view.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Example context (json):
    {
    }
}}
<div id="calendar-month-{{uniqid}}" data-template="mod_virtualcoach/week_detailed">
    {{> mod_virtualcoach/week_detailed}}
</div>
{{#js}}
require(['jquery', 'core_calendar/calendar', 'mod_virtualcoach/view_manager', 'mod_virtualcoach/fixed_day_names'], function($, Calendar, ViewManager, FixedDayNames) {
    root = $("#calendar-month-{{uniqid}}");
    Calendar.init(root);
    ViewManager.init(root);
    FixedDayNames.init(root);
});
{{/js}}
